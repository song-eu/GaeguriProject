{
   "name": "gaeguri-server",
   "version": "0.0.1",
   "description": "gaeguri",
   "main": "src/index.ts",
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/cors": "^2.8.6",
      "@types/faker": "^4.1.12",
      "@types/glob": "^7.1.2",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/morgan": "^1.9.1",
      "@types/multer": "^1.4.3",
      "@types/node": "^8.0.29",
      "@types/passport": "^1.0.3",
      "@types/passport-local": "^1.0.33",
      "gql2ts": "^1.10.1",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@babel/preset-env": "^7.10.4",
      "bcrypt": "^5.0.0",
      "bcryptjs": "^2.4.3",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express-mysql-session": "^2.1.4",
      "express-session": "^1.17.1",
      "glob": "^7.1.6",
      "graphql-depth-limit": "1.1.0",
      "graphql-import": "^1.0.2",
      "graphql-middleware": "^4.0.2",
      "graphql-tools": "^6.0.9",
      "graphql-yoga": "^1.18.3",
      "jsonwebtoken": "^9.0.0",
      "merge-graphql-schemas": "^1.7.8",
      "moment": "^2.27.0",
      "morgan": "^1.10.0",
      "multer": "^1.4.2",
      "mysql": "^2.14.1",
      "nodemon": "^2.0.4",
      "passport": "^0.4.1",
      "passport-google-oauth20": "^2.0.0",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.25",
      "typeorm-seeding": "^1.6.1"
   },
   "resolutions": {
      "graphql": "^15.1.0"
   },
   "scripts": {
      "start": "nodemon --exec ts-node src/index.ts",
      "gen-schema-type": "gql2ts src/api/**/*.graphql -o src/types/**/*.schema.d.ts",
      "type": "ts-node src/types/createTypes.ts",
      "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
      "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "deploy-development": "pm2 deploy ecosystem.config.js development --force",
      "reload-development": "pm2 deploy ecosystem.config.js development exec 'pm2 reload app'"
   }
}
